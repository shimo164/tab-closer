{
	"name": "Tab Closer",
	"description": "Automatically close unwanted tabs using custom keywords and a user-defined timer. ",
	"version": "1.0",
	"manifest_version": 3,
	"options_page": "option.html",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
